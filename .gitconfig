[user]
	name = Edmilson Silva
	email = edmilson@denakop.com
[alias]
	s = status
	br = branch
	ck = checkout
	co = commit
	unstage = reset HEAD --
	lg = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
	email = config --get-all user.email
	name = config --get-all user.name
[pull]
	ff = true
